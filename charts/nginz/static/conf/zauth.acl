             (regex "(/v[0-9]+)?/bot/conve[^r].*")
             (regex "(/v[0-9]+)?/bot/conver[^s].*")
             (regex "(/v[0-9]+)?/bot/convers[^a].*")
             (regex "(/v[0-9]+)?/bot/conversa[^t].*")
             (regex "(/v[0-9]+)?/bot/conversat[^i].*")
             (regex "(/v[0-9]+)?/bot/conversati[^o].*")
             (regex "(/v[0-9]+)?/bot/conversatio[^n].*")
             (regex "(/v[0-9]+)?/bot/conversation[^/].+")
