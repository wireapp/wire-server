a (blacklist (regex "(/v[0-9]+)?/provider(/.*)?")
             (regex "(/v[0-9]+)?/bot/[^c].*")
             (regex "(/v[0-9]+)?/bot/c[^o].*")
             (regex "(/v[0-9]+)?/bot/co[^n].*")
             (regex "(/v[0-9]+)?/bot/con[^v].*")
